{
  "name": "com.quasar.grolocation.app.com",
  "displayName": "Quasar geolocation Application",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the device-ready event.",
  "main": "index.js",
  "scripts": {
    "dev": "npx quasar dev -m cordova -T android",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^13.0.0",
    "cordova-plugin-geolocation": "^5.0.0",
    "cordova-plugin-permission": "^0.1.0",
    "cordova-plugin-request-location-accuracy": "^2.3.0"
  },
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      },
      "cordova-plugin-request-location-accuracy": {
        "PLAY_SERVICES_LOCATION_VERSION": "16.+"
      },
      "cordova-plugin-permission": {}
    }
  }
}